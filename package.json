{
  "name": "@eversource/storybook",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://dev.azure.com/Eversource-Energy/ms-benchmark/_git/storybook?path=/README.md",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "dependencies": {
    "@carbon/react": "1.46.1",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@floating-ui/react": "^0.26.23",
    "@mui/icons-material": "5.15.2",
    "@mui/material": "5.15.2",
    "@storybook/addon-a11y": "7.6.6",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "react-jsx-parser": "^2.0.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "build-storybook": "storybook build",
    "build-npm-package": "rollup -c",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test --watchAll=false --watch=false --no-watchman --coverage --env=jsdom --testPathIgnorePatterns=assets --reporters=default --reporters=jest-junit --coverageReporters=cobertura --forceExit --passWithNoTests",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "storybook": "storybook dev --https --ssl-cert cert/localhost.pem --ssl-key cert/localhost-key.pem -p 6006",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "lint:js": "npx eslint \"./src/**/*.js\" --cache --cache-strategy content",
    "lint:js:fix": "npx eslint \"./src/**/*.js\" --cache --cache-strategy content --fix",
    "publish:ci": "npm publish"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/src/App.js",
      "/src/index.js",
      "/src/reportWebVitals.js",
      "/src/components/Wizard/children.js",
      ".stories.js",
      "/src/stories"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/preset-react": "7.23.3",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-image": "3.0.3",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@storybook/addon-designs": "7.0.7",
    "@storybook/addon-docs": "7.6.7",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/blocks": "7.6.6",
    "@storybook/manager-api": "7.6.6",
    "@storybook/preset-create-react-app": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/react-webpack5": "7.6.6",
    "@storybook/test": "7.6.6",
    "@svgr/rollup": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "babel-loader": "9.1.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.6.15",
    "jest-junit": "16.0.0",
    "prettier": "3.2.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "2.79.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "storybook": "7.6.6",
    "stylelint": "16.1.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-order": "6.0.4",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
